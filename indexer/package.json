{
  "name": "indexer",
  "version": "0.0.1",
  "packageManager": "yarn@4.1.1",
  "type": "module",
  "scripts": {
    "build": "rm -rf ./build && tsc && cd ./src && cp --parents **/*.py ../build/",
    "dev": "yarn build && node build/index.js",
    "typecheck": "tsc --noEmit",
    "test": "poetry run vitest"
  },
  "imports": {
    "#mocks": "./__tests__/utils/index.ts"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@types/node": "20.12.7",
    "@types/pg": "^8",
    "typescript": "5.4.5",
    "vitest": "1.5.0"
  },
  "dependencies": {
    "node-html-parser": "6.1.13",
    "pg": "8.11.5",
    "python-shell": "5.0.0"
  }
}
