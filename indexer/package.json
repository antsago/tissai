{
  "name": "@tissai/indexer",
  "version": "0.0.1",
  "packageManager": "yarn@4.1.1",
  "type": "module",
  "scripts": {
    "start": "poetry run node build/index.js",
    "build": "rm -rf ./build && tsc && cd ./src && cp --parents **/*.py ../build/",
    "typecheck": "tsc --noEmit",
    "test": "poetry run vitest run",
    "test:integration": "poetry run vitest run __tests__",
    "format": "yarn format:js & yarn format:py",
    "format:js": "prettier . --write",
    "format:py": "poetry run black ."
  },
  "imports": {
    "#mocks": "./__tests__/utils/index.ts"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@types/he": "^1",
    "@types/lodash.defaults": "^4",
    "@types/node": "20.12.7",
    "@types/pg": "^8",
    "@types/pg-cursor": "^2",
    "prettier": "3.2.5",
    "typescript": "5.4.5",
    "vitest": "1.5.0"
  },
  "dependencies": {
    "he": "1.2.0",
    "lodash.defaults": "4.2.0",
    "node-html-parser": "6.1.13",
    "ora": "8.0.1",
    "pg": "8.11.5",
    "pg-cursor": "2.10.5",
    "python-shell": "5.0.0"
  }
}
